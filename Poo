local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "The One Panel | WhaleScripts", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local KTab = Window:MakeTab({
	Name = "Killer",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

KTab:AddParagraph("Reminder","if you like ruining peoples day and making them endlessy shit their diaper and say "HACKER!!" ")

KTab:AddButton({
	Name = "Kill All [COLLISION ONLY]",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/zqyDSUWX"))()
  	end    
})

OrionLib:MakeNotification({
	Name = "Not Alright?",
	Content = "those coolkidd's wanna bes need a require lmao",
	Image = "rbxassetid://4483345998",
	Time = 10
})

KTab:AddParagraph("Annoying Shit","HEY HEY HEY DUDE HEY DUDE GUESS WHAT HEY DUDE!")

-- Variable to keep track of the toggle state
local toggleState = false

-- Function to make the character say "WHALESCRIPTS" in a loop
local function chatWhaleScripts()
    while toggleState do
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("WHALESCRIPTS", "All")
        wait(2)
    end
end

-- Toggle callback function
local function onToggle(state)
    toggleState = state
    if toggleState then
        chatWhaleScripts()
    end
end

KTab:AddToggle({
	Name = "Spam Chat [My Name]",
	Default = false,
	Callback = onToggle
	end    
})

OrionLib:Init()
